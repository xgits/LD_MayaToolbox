}
QTabWidget::pane {
    border: None;
    background: black;
}
QTabBar::tab {
    background-color: rgb(80,80,80); 
	width:50px;
	height: 22px;
	color: rgb(50,50,50);
	padding: 0 0 7px 2px;
	margin: 0 0 -8px 0;
	border-radius: 6px;
	
}

QTabBar::tab:hover {
	color: rgb(200,200,200);
	background-color: rgb(85,85,85); 
	text-decoration: underline!important;
}

QTabBar::tab:selected {
    background-color: rgb(95,95,95); 
	height: 22px;
	color: rgb(200,200,200);
	width:50px;
}

*.QGroupBox {
    background-style:None;
	margin-top:6px;
	border:1px solid rgb(145,145,145);
	padding: 2px 1px 1px 1px;
	border-radius: 6px;
}

*.QGroupBox::title {
    background-style: None;
	top:-9px;
	subcontrol-position: top center;
	color: rgb(145,145,145);
}

*{background-color: rgb(95,95,95);}

QPushButton{color: rgb(0,0,0);
background-color: qlineargradient(spread:pad, x1:1, y1:1, x2:1, y2:0, stop:0 rgba(15, 15, 15, 125), stop:1 rgba(55,55, 55, 55));
border-style: solid;
border-width: 1px;
border-radius: 4px;
}

QToolButton { /* all types of tool button */
    border-style: None;
	background-style:None;
	padding-left : 6px;
	padding-bottom : 1px;
	color:rgb(185,185,185);
}

*.QToolButton:pressed{
	background-color: qlineargradient(spread:pad, x1:1, y1:1, x2:1, y2:0, stop:0 rgba(55, 55, 55, 125), stop:1 rgba(15,15, 15, 55));
}
QToolButton:hover {
    border: 1px solid rgb(120,120,120);
    border-radius: 6px;
	background-color: qlineargradient(spread:pad, x1:1, y1:1, x2:1, y2:0, stop:0 rgba(15, 15, 15, 125), stop:1 rgba(55,55, 55, 55));
	padding-left : 6px;
	padding-bottom : 1px;
	color:rgb(200,200,200);
	text-decoration: underline!important;
}

QToolButton::down-arrow:on {
	left:10px;
}

